<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/bg"
       />

    <ImageButton
        android:id="@+id/buttonback"
        android:layout_width="100px"
        android:layout_height="100px"
        android:scaleType="fitXY"
        android:background="#00000000"
        android:layout_gravity="center_vertical"
        />
    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_alignParentStart="true"
        android:layout_marginStart="122px"
        android:layout_marginTop="50px"
        android:background="#00000000"
        android:scaleType="fitXY"

        android:src="@drawable/icon_youhui" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90px"
        android:layout_marginTop="50px"
        android:layout_toEndOf="@+id/imageButton6"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_shenbao" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90px"
        android:layout_marginTop="50px"
        android:layout_toEndOf="@+id/imageButton7"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_fapiao" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90px"
        android:layout_marginTop="50px"
        android:layout_toEndOf="@+id/imageButton8"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_dengji" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_alignParentStart="true"
        android:layout_marginStart="343px"
        android:layout_marginTop="427px"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_zhengming" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_marginStart="90px"
        android:layout_marginTop="427px"
        android:layout_toEndOf="@+id/imageButton10"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_tongzhi" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="351px"
        android:layout_height="400px"
        android:layout_marginStart="90px"
        android:layout_marginTop="427px"
        android:layout_toEndOf="@+id/imageButton11"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/icon_redian" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="480px"
        android:layout_height="100px"
        android:layout_below="@+id/imageButton11"
        android:layout_alignParentStart="true"
        android:layout_marginStart="710px"
        android:layout_marginTop="20px"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:src="@drawable/btn__quhao" />
</RelativeLayout>